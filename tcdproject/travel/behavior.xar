    def headDown(self):
        names = []
        times = []
        keys = []
        names.append("HeadPitch")
        times.append([1.24])
        keys.append([[0.392662, [3, -0.413333, 0], [3, 0, 0]]])

        names.append("HeadYaw")
        times.append([1.24])
        keys.append([[-0.027654, [3, -0.413333, 0], [3, 0, 0]]])
